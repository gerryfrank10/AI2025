    "     \"start_time\": \"2025-04-05T02:15:01.139998Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"from torch.utils.data import Dataset\\n\",\n",
    "    \"\\n\",\n",
    "    \"class JointDataset(Dataset):\\n\",\n",
    "    \"    def __init__(self, x, y):\\n\",\n",
    "    \"        super().__init__()\\n\",\n",
    "    \"        self.x = x\\n\",\n",
    "    \"        self.y = y\\n\",\n",
    "    \"    def __len__(self):\\n\",\n",
    "    \"        return len(self.x)\\n\",\n",
    "    \"    def __getitem__(self, idx):\\n\",\n",
    "    \"        return self.x[idx], self.y[idx]\"\n",
    "   ],\n",
    "   \"id\": \"9d198f5b8a64f0ae\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 43\n",
